{
  "name": "edgedb-stuff",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn generate && yarn compile && next build",
    "start": "next start",
    "compile": "bun antlr && bun ./compiler/compiler.ts",
    "lint": "next lint",
    "antlr": "rm -rf antlr/* && java -jar ./antlr.jar -Dlanguage=TypeScript -o antlr -visitor ./MyGrammar.g4",
    "generate": "bunx @edgedb/generate edgeql-js",
    "migrate": "edgedb database wipe --non-interactive && edgedb migrate && edgedb migration create --non-interactive --allow-unsafe && edgedb migrate; bun generate"
  },
  "dependencies": {
    "@faker-js/faker": "^8.2.0",
    "@heroicons/react": "^2.0.18",
    "@total-typescript/ts-reset": "^0.5.1",
    "antlr4": "^4.13.1-patch-1",
    "chalk": "^5.3.0",
    "date-fns": "^2.30.0",
    "fast-glob": "^3.3.2",
    "next": "^14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "ts-morph": "^20.0.0"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.3.4",
    "@types/glob-watcher": "^5.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "bun-types": "^1.0.13",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "glob-watcher": "^6.0.0",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
